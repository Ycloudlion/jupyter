get_ipython().getoutput("pip install pandas")



import pandas as pd

df = pd.read_excel('customers.xlsx')
print(f'Filas: {len(df)}, Columnas: {len(df.columns)}')
df.head()



# Filtrar usuarios con correo electrónico
df_con_email = df[df['EMAIL_ADRS'].notna() & (df['EMAIL_ADRS'] != '')]
print(f'Usuarios con email: {len(df_con_email)}')
df_con_email.head()



# Verificar columnas relacionadas con email
email_columns = [col for col in df.columns if 'EMAIL' in col.upper()]
print('Columnas relacionadas con email:')
for col in email_columns:
    print(f'- {col}')



# Filtrar usuarios con correo electrónico (usando RPT_EMAIL)
df_con_email = df[df['RPT_EMAIL'].notna() & (df['RPT_EMAIL'] != '') & (df['RPT_EMAIL'] != '0')]
print(f'Usuarios con email: {len(df_con_email)}')
df_con_email.head()




